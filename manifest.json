{
    "manifest_version": 2,
    "name": "Episode Tracker",
    "description": "Extension that tracks which episode of a series you are on.",
    "version": "0.1",
    "author": "Derek Morey",
    "browser_action": {
        "default_icon": {
            "19": "assets/icon19.png",
            "38": "assets/icon38.png"
        },
        "default_popup": "popup.html",
        "default_title": "Track Episodes"
    },
    "permissions": [
        "tabs", 
        "storage"
    ]
}
