{
    "manifest_version": 2,
    "name": "Anime Tracker",
    "description": "Extension that tracks which episode of an anime you're on.",
    "version": "0.1.0",
    "author": "Derek Morey",
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "assets/icon19.png",
            "38": "assets/icon38.png"
        },
        "default_popup": "popup.html",
        "default_title": "Track Episodes"
    },
    "permissions": [
        "tabs", 
        "storage"
    ]
}
